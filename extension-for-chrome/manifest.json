{
    "manifest_version": 2,
    "name": "gatric",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                
                "https://gartic.com.br/*"
                
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ],

    "browser_action": {
        "default_popup": "ext.html",
        "default_icon": "g.png"
                       },

"commands": {
     "_execute_browser_action": {
       "suggested_key": {
         "default": "Ctrl+Shift+F",
         "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens gtc.html"
        }
      }
     

      
}